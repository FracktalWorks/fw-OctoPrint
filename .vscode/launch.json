  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "python",
      "request": "launch",
      "pythonPath": "${env:OCTOPRINT_ENV}\\Scripts\\python.exe",
      "env": {
        "PYTHONPATH": "${workspaceFolder}\\src;${env:OCTOPRINT_ENV}\\lib\\site-packages"
      },
      "program": "${workspaceFolder}\\src\\octoprint",
      "args": ["serve", "-b.octoprint"],
      "console": "integratedTerminal"
    },
  ]
}